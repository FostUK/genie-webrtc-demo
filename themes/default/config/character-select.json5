{
    theme: {
        "character-select": {
            rows: 1,
            columns: 1,
            ease: "Cubic.easeInOut",
            duration: 500,
            align: "center",
            music: "loader.backgroundMusic",
            titles: [
                {
                    type: "image",
                    key: "title",
                    xOffset: 0,
